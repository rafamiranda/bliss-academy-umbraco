{
  "$schema": "./umbraco/config/appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  },
  "ConnectionStrings": {
    "umbracoDbDSN": "Data Source=(localdb)\\MSSQLLocalDB;AttachDbFilename=|DataDirectory|\\Umbraco.mdf;Integrated Security=True"
  },
  "Umbraco": {
    "CMS": {
      "Hosting": {
        "Debug": false
      },
      "Content": {
        "ContentVersionCleanupPolicy": {
          "EnableCleanup": true
        },
        "MacroErrors": "Inline"
      },
      "Global": {
        "Id": "0cbbda48-29f1-40eb-9a0b-bbb88f5e347c"
      },
      "ModelsBuilder": {
        "ModelsMode": "SourceCodeManual",
        "ModelsNamespace": "BlissAcademy.Core.PublishedContent",
        "ModelsDirectory": "~/../../Projects/BlissAcademy.Core/PublishedContent",
        "AcceptUnsafeModelsDirectory": true,
        "DebugLevel": 1
      },
      "WebRouting": {
        "TrySkipIisCustomErrors": true
      }
    }
  }
}